<h1>Edit board game</h1>
<div>
  <%= simple_form_for @game, url: admin_board_game_path(@game), method: :patch do |f| %>
    <%= f.input :name, required: true %>

    <%= f.input :description, required: true %>

    <%= f.input :video %>

    <%= f.input :reference %>

    <%= f.input :image, required: true %>

    <%= f.submit "Save" %>
    <% end %>
</div>
