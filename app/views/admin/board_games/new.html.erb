<h1>Add board game</h1>
<div>
  <%= simple_form_for @game, url: admin_board_games_path, method: :post do |f| %>
    <%= f.input :name, required: true %>

    <%= f.input :description, required: true %>

    <%= f.input :video %>

    <%= f.input :reference %>

    <%= f.input :image, required: true %>

    <%= f.submit "Create" %>
    <% end %>
</div>
